.case{
	padding:70px 0;
	text-align:center;
	background:#f9f9f9;
}
.case .title{
	margin:0 0 50px;
	font-size:36px;
}
.case ul li{
	width:230px;
	margin:15px 10px;
	display:inline-block;
	font-size: 15px;
}
.case ul li .spin {
  height: 300px;
  transition: 0.5s;
  transform-style: preserve-3d;
  position: relative;
	box-shadow: 0 3px 5px 0 #ccc;
}
.case ul li:hover .spin{
  transform: rotateY(180deg);
  -ms-transform: rotateY(180deg);
  -moz-transform: rotateY(180deg);
  -webkit-transform: rotateY(180deg);
  -o-transform: rotateY(180deg);
}
.case ul li .spin .just, .case ul li .spin .instead {
  backface-visibility: hidden;
  -ms-transform: rotateY(0deg);
  -moz-transform: rotateY(0deg);
  -webkit-transform: rotateY(0deg);
  position: absolute;
}
.case ul li .spin .just {
  z-index: 2;
  border-bottom: none;
}
.case ul li .spin .just p{
	width: 230px;
  height: 250px;
}
.case ul li .spin .just .span {
  display: block;
  width: 230px;
  height: 50px;
  background: #00a3ff;
  text-align: center;
  line-height: 50px;
  margin-top: 0px;
  color:#fff;
}
.case ul li .spin .instead {
  transform: rotateY(180deg);
  width: 230px;
  height: 250px;
  background: #242424;
  transition: 0.5s ease-out;
}
.case ul li .spin .instead .img {
  width: 230px;
  height: 250px;
}
.case ul li .spin .instead .img img {
	width: 150px;
	height: 150px;
	margin: 50px 40px;
}
.case ul li .spin .instead .span {
  display: block;
  width: 230px;
  height: 50px;
  background: #fff;
  text-align: center;
  line-height: 50px;
  margin-top: 0px;
  color:#00a3ff;
}
