header{
  position: fixed;
  top: 0;
  z-index:999;
  width: 100%;
  height:65px;
  transition:All 0.4s ease;
	-webkit-transition:All 0.4s ease;
	-moz-transition:All 0.4s ease;
	-o-transition:All 0.4s ease;
}
header .nav .logo{
	display:block;
	width:300px;
	height:50px;
	margin-left:20px;
	margin-top: 7px;
	background:url(/qrcode/ig/logo-white.png) no-repeat center;
	background-size:contain;
}
header.fix .nav .logo{
	background:url(../../qrcode/ig/logo.png) no-repeat center;
	background-size:contain;
}
header .nav .navbar{
	text-align:right;
	padding:0 20px;
}
header.fix .nav .navbar{
	padding:7px 20px;
}
header .nav .navbar .menu{
	display:none;
	margin:16px 0 17px;
}
header.fix .nav .navbar .menu{
  margin: -16px 0 -17px;
  padding:16px 0 17px;
}
header .nav .navbar .menu li{
	display:inline-block;
	text-align:center;
	position:relative;
	margin-left: 5px;
}
header .nav .navbar .menu li:hover{
	background-color: rgba(255,255,255,.1);
}
header.fix .nav .navbar .menu li.list{
	margin: -16px 0 -17px;
  padding:16px 0 17px;
	margin-left: 5px;
}
header .nav .navbar .menu li a{
	color:#fff;
	font-size: 18px;
	line-height:20px;
    padding: 6px 15px;
    border-radius: 3px;
	display:inline-block;
}
header .nav .navbar .menu li .a2{
	color:#363636;
	font-size: 18px;
	line-height:20px;
	padding: 6px 15px;
	border-radius: 3px;
	display:inline-block;
}
header .nav .navbar .menu li a:hover{
	background-color: rgba(255,255,255,.2);
}
header.fix .nav .navbar .menu li a, header.fix .nav .navbar .menu li dl.active a{
	color: #222;
}
header.fix .nav .navbar .menu li a:hover, header.fix .nav .navbar .menu li.active a, header.fix .nav .navbar .menu li dl.active a:hover, header.fix .nav .navbar .menu li .a2:hover{
	color: #00a3ff;
}
header .nav .navbar .menu li dl{
	display:none;
	width:100%;
	position: absolute;
	border-radius: 0 0 3px 3px;
  background-color: rgba(255,255,255,.1);
}
header.fix .nav .navbar .menu li dl,header.fix{
	background-color: rgba(255,255,255,.98);
	-webkit-box-shadow: 0 3px 10px rgba(0,0,0,.1);
  -moz-box-shadow: 0 3px 10px rgba(0,0,0,.1);
  box-shadow: 0 3px 10px rgba(0,0,0,.1);
}
header.fix .nav .navbar .menu li dl{
	top:60px;
}
header .nav .navbar .menu li:hover dl{
	display:block;
}
header .nav .navbar .menu li dl a{
	display: block;
  font-size: 16px;
  padding: 6px 0;
  line-height: 20px;
	border-radius:0;
}
header .nav .navbar .menu li dl a:hover{
	background-color: rgba(255,255,255,.2);
}
header .nav .nav_button{
	display:inline-block;
}
header .nav .nav_button a{
	color:#fff;
	font-size:15px;
	border-radius: 2px;
	display:inline-block;
	margin: 10px 4px;
}
header .nav .nav_button .login{
	border: 1px solid #fff;
	padding: 5px 20px;
	background-color: transparent;
}
header .nav .nav_button .login2 {
    border: 1px solid #000;
	padding: 5px 20px;
	color:#363636;
	background-color:transparent;
}
header .nav .nav_button .login:hover{
	color: #00a3ff;
  background-color: #fff;
}
header.fix .nav .nav_button .login{
	color: #555;
  border: 1px solid #555;
  background-color: transparent;
}
header.fix .nav .nav_button .login:hover{
	color: #fff;
  background-color: #555;
}
header .nav .nav_button .registered{
	padding: 6px 20px;
	position:relative;
  background-color: #00a3ff;
}
header .nav .nav_button .registered:hover{
	background-color: #00b7ff;
}
header .nav .nav_button .registered span{
	position:relative;
	cursor: pointer;
}
header .nav .nav_button .registered:hover:before{
	opacity: 1;
}
.banner{
	width:100%;
	height:620px;
	position:relative;
}
.banner .swiper-container,.banner .swiper-container .swiper-wrapper,.banner .swiper-container .swiper-slide{
  width: 100%;
  height: 100%;
	margin: 0 auto;
  position: relative;
  overflow: hidden;
}
.banner .swiper-container{
	position:absolute;
	top:0;
	left:0;
}
.banner .swiper-container .swiper-slide {
  background-position: center top !important;
  background-size: cover !important;
}
.banner .swiper-container .swiper-slide a{
	display: inline-block;
  font-size: 16px;
  margin: 0 20px;
