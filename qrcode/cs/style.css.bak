.topnav-menu-col .topnav-rotate-middle>span {
 top:22px
}
.topnav-menu-col .topnav-rotate-bottom>span {
 top:32px
}
.topnav-toggle .topnav {
 height:100%
}
.topnav-toggle .topnav-rotate-top {
 transition:all .25s ease;
 -webkit-transform:rotatez(45deg) translateY(15px) translateX(-1px);
 -ms-transform:rotate(45deg) translateY(15px) translateX(-1px);
 transform:rotatez(45deg) translateY(15px) translateX(-1px)
}
.topnav-toggle .topnav-rotate-middle {
 transition:all .25s ease;
 -webkit-transform:rotatez(-45deg) translateY(0) translateX(-4px);
 -ms-transform:rotate(-45deg) translateY(0) translateX(-4px);
 transform:rotatez(-45deg) translateY(0) translateX(-4px)
}
.topnav-toggle .topnav-rotate-bottom {
 transition:all .25s ease;
 display:none
}
