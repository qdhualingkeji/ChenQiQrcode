}
.about {width: 100%;float: none;}
.aboutl {width: 100%;}
.about h2, .aboutl h2 {
    font-size: .4rem;
    text-align: center;
    padding: .2rem .1rem;
}
.about p, .aboutl p {
    font-size: .3rem;
    text-align: left;
    padding: .1rem 0 0 .2rem;
}
/* 在线生成 */
#title{padding-top:1.6rem;padding-bottom:.6rem;font-size:.48rem;color:#555;text-align: center;}
ul#tab, .box, form {margin-left:auto;margin-right:auto;}
ul#tab {font-size:.32rem;list-style-type:none;overflow:hidden;margin-top:0px;width:95%;}
body,ul#tab li {margin:0px;padding:0px;}
ul#tab li{float:left;width:33.3%;text-align:center;border-bottom:2px solid #fff;padding-top:2px;padding-bottom:.3rem;}
ul#tab li.on{border-bottom:2px solid #00a3ff;}
ul#tab li.on a{color:#00a3ff;}
.box{text-align:center;}
textarea{height:3rem;border:1px solid #d4d0c8;border-radius:3px;}
textarea,input{-webkit-appearance:none;width:91%;outline:0;padding:.14rem;font-size:.28rem;}
input{border:none;}
textarea::-webkit-input-placeholder, input::-webkit-input-placeholder {
	font-family: 'Microsoft YaHei';font-size:1rem;
}
textarea:-ms-input-placeholder, input:-ms-input-placeholder { // IE10+
	font-family: 'Microsoft YaHei';font-size:1rem;
}
textarea:-moz-input, input:-moz-input { // Firefox4-18
	font-family: 'Microsoft YaHei';font-size:1rem;
}
textarea::-moz-input, input::-moz-input { // Firefox19+
	font-family: 'Microsoft YaHei';font-size:.3rem;
}
table {
	clear:both;
	border-color:#e1e1e1;
	border-width:1px;
	border-style:none;
	width:95%;
	border-collapse:collapse;
	font-size: .23rem; 
	border-bottom:1px solid #e1e1e1;
    color:#333;
    margin:auto;
    line-height:1.5em;
}
table thead th, table tbody td { padding: .08rem; border: 1px solid #e1e1e1;  }
table thead th { background:#f1f1f1;color:#aaa;text-align:center;}
td.t{font-size: .3rem;}
#btsubmit{padding:.21rem;width:93%;margin-top:.5rem;font-size:.32rem;}
#progress{margin-bottom:1rem;font-size:.28rem;}
#tooltip #qrclose{top:.3rem;font-size:.3rem;}
#qrclose a{font-size:.9rem;}
textarea::-webkit-input-placeholder,input::-webkit-input-placeholder {
         color: #888;
         font-size: .28rem;
}
