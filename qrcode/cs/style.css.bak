  margin-left: 0;
  width:auto;
  text-align: right;
  padding-right: 40px;
	z-index:9;
}
.banner .swiper-container .swiper-pagination span{
  width: 10px;
  height: 10px;
  margin: 1px;
	opacity:1;
	margin-left:14px;
