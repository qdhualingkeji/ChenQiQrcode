<script>
function createQrcode(){
	var url=$("#url_inp").val();
	$.ajax({
		url:"http://www.bainuojiaoche.com:8080/GoodsPublic/merchant/main/createQrcodeByCQSCQ",
		data:{url:url},
		dataType:"jsonp",
		type: "post",
		jsonpCallback:"jsonpCallback",
		success:function(data){
			var qrcode=JSON.parse(data).qrcode;
			$("#showQrcode_div").empty();
			$("#showQrcode_div").append("<img src=\"http://www.bainuojiaoche.com:8080"+qrcode+"\"/>");
		}
	});
}
</script>
