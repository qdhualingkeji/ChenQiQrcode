	var questionType = 1;

	var RECORD_BLOG_CAT_ID = '570';
	var BATCH_BLOG_CAT_ID = '526';
	var SCENE_BLOG_CAT_ID = '556';
	var BUY_BLOG_CAT_ID = '559';
	var currentCategoryId = '504';
	var currentPostId = 48446;
	var helpQqUrl = '';

	var isWorkTime = $ServiceTextGetter().special.isWorkTime();
	var isWorkTimeFull = $ServiceTextGetter().special.isWorkTimeFull ? $ServiceTextGetter().special.isWorkTimeFull() : isWorkTime;
	var todayIsDayOff = $ServiceTextGetter().special.todayIsDayOff();
	var $imagesContent = $('.images-content');

	var isMac = function() { 
		return /macintosh|mac os x/i.test(navigator.userAgent); 
	}

	function debounce(func, wait, immediate) {
		var timeout;
		return function() {
		var context = this, args = arguments;
		var later = function() {
			timeout = null;
			if (!immediate) func.apply(context, args);
		};
		var callNow = immediate && !timeout;
		clearTimeout(timeout);
		timeout = setTimeout(later, wait);
		if (callNow) func.apply(context, args);
		};
	};

	function throttle(fn, delay) {
		var timer;
		return function() {
			var last = timer;
			var now = Date.now();
			if(!last) {
				timer = now;
				fn.apply(this,arguments);
				return;
			}
			if(last + delay > now) return;
			timer = now;
			fn.apply(this,arguments);
		}
	}

	function showError(msg) {
		$('#error-message')
			.text(msg)
			.fadeIn()
			.delay(2000)
			.fadeOut();
  	}

	var newUplode = function (obj, param) {
		obj.upload({
				exts: param.exts, //支持的扩展名
				extsfun: function (nowexts) { //不支持的扩展名回调
				showError('不支持该扩展名：' + nowexts);
			},
			maxsize: param.maxsize, //最大2MB
			maxsizefun: function (nowsize) { //超出大小回调
				showError("最大支持2MB，现在" + nowsize);
			},
			complete: function (data) { //上传成功
				if ($imagesContent.children().length >= 5) {
					$imagesContent.find('.add-content').addClass('none');
				}
				imagefile.push(data.path);
				$('.add-content').before('<div class="image-content">' +
					'<span class="inner-image text-darkgrey">' + data.name + '</span>' +
					'<a class="text-blue m-l-sm close-image">删除</a>' +
					'</div>');
				$('.images-content').find('.close-image').unbind('click').click(function (event) {
					var index = $('.images-content').find('.close-image').index($(this));
					$(this).parent().remove();
					imagefile.splice(index, 1);
					if ($imagesContent.children().length < 5) {
						$imagesContent.find('.add-content').removeClass('none');
					}
				});
			},
			failed: function (msg) { //上传失败
				showError('上传失败');
			}
		});
	};

	function StatisticsDataWithComment(fir, sec, comment) {
		$.ajax({
			async: true,
			type: "get",
			url: '/Api/ClickLog/click',
			data: {
				fir: fir,
				sec: sec,
				comment: comment
			},
			dataType: 'jsonp',
			jsonp: 'callback'
		});
	}

	/* socket */
	var socket = new Object();
	// 初始化websocket
	function _initWbSocket(ssid, msg_name, callback) {
		if (!socket.io) {
		//连接服务端
			socket.io = io('https://socket.api.cli.im', {
				'force new connection': true
			});
		} else {
			if (!socket.io.connected) {
				socket.io.connect();
			}
		}
		//socket连接后以uid登录
		socket.io.on('connect', function() {
			socket.io.emit('login', 'weixin_ask_' + ssid);
		});
		//后端推送来消息时
		socket.io.on(msg_name, function(req) {
			if (isJson(req)) {
				req = JSON.parse(req);
			}
			callback(req);
		});
	}

	/* 判断是不是 json 字符串*/
	function isJson(str) {
		if (typeof str == 'string') {
			try {
				JSON.parse(str);
				return true;
			} catch (e) {
				return false;
			}
		}
	}

	function initSupport() {
		var noneClass = $('#main').height() > $('#primary').height() ? ' none' : '';
		var supportWrapper = $('<a class="support-wrapper-v3' + noneClass + '" href="javascript:;" id="support-wrapper-v3" target="_blank">问题未解决</a>');
		if ($('#support-wrapper-v3').length == 0) {
			supportWrapper.appendTo('body');
			reRenderSupport(currentCategoryId);
        }
	}

	function changeIcon(type) {
		if (type == 'qq' && isWorkTimeFull) {
			$('#support-wrapper-v3').css({
				'background-image': 'url(https://static.clewm.net/cli/images/help-qq-icon.png)',
				'background-size': '30px',
			});
		} else {
			$('#support-wrapper-v3').css({
				'background-image': 'url(https://static.clewm.net/cli/images/help-icon@2x.png)',
				'background-size': '28px',
			});
		}
	}

	function reRenderSupport(catId) {
		switch (catId) {
			case RECORD_BLOG_CAT_ID:
				changeIcon('qq');
				helpQqUrl = 'http://q.url.cn/cdlf3l?_type=wpa&qidian=true';
				break;
			case BATCH_BLOG_CAT_ID:
				changeIcon('qq');
				helpQqUrl = 'http://q.url.cn/ABTMel?_type=wpa&qidian=true';
				break;
			case SCENE_BLOG_CAT_ID:
				changeIcon('qq');
				helpQqUrl = 'http://q.url.cn/CDf2rS?_type=wpa&qidian=true';
				break;
			case BUY_BLOG_CAT_ID:
				changeIcon('qq');
				helpQqUrl = 'http://q.url.cn/ABeWol?_type=wpa&qidian=true';
				break;
			default:
				var currentEditionId = getCookie('login_edition_id') || '1';
				if (currentEditionId > 1) {
					changeIcon('qq');
					if (currentEditionId == 44 || currentEditionId == 4) {
						helpQqUrl = 'http://q.url.cn/CDsY7l?_type=wpa&qidian=true';
					} else {
						helpQqUrl = 'http://q.url.cn/abYhOU?_type=wpa&qidian=true';
					}
				} else {
					changeIcon('');
					helpQqUrl = '';
				}
				break;
		}
	}

	function getWechatImage() {
		$.ajax({
			type: 'GET',
			url: '//' + USER_DOMAIN + '/api/ask/isSubscribed',
			dataType: 'json',
			xhrFields: {
				withCredentials: true
			},
			success: function(res) {
				if (res.status == 1) {
					$('#wechat-qr').attr('src', res.data.qrcode_img);
					is_subscribed = res.data.is_subscribed;
					if (res.data.is_subscribed) {
						$('#wechat-area').addClass('none');
					} else {
						$('#wechat-area').removeClass('none');
					}
					if (res.data.ssid && !res.data.is_subscribed) {
						if (socket.io) {
							socket.io.disconnect(true);
						}
						_initWbSocket(res.data.ssid, 'weixin_ask_msg', function(message_obj) {
							is_subscribed = true;
							$('.scan-success').removeClass('none');
						});
					}
				}
			},
			error: function() {
				alert('加载失败');
			}
		});
  	}

	function submitBtn(open, contactId) {
		// var theme=$('#theme').val();
		var content = $('#iptArea').val();
		var currentType = questionType;
		var ct_email = $("#email").val();
		var reg = /^([a-zA-Z0-9]+[_|\_|\.]?)*[a-zA-Z0-9]+@([a-zA-Z0-9]+[_|\_|\.]?)*[a-zA-Z0-9]+\.[a-zA-Z]{2,3}$/; //判断用户是不是输入正确的邮箱\
		
		if (!submitType) {
			showError('请选择回复方式');
			return false;
		}
		if (submitType === 'wechat' && !is_subscribed) {
			showError('请先关注公众号');
			return false;
		}

		if (submitType === 'email' && !reg.test(ct_email)) {
			showError('请输入正确邮箱');
			return false;
		}
		if (currentType) {
			var sendimagefile = imagefile.join(",");
			if (!isWorkTime) {
				if (isWorkTimeFull) {
					contactId = todayIsDayOff ? 10 : 9;
				} else {
					contactId = 11;
				}
			}

			$.ajax({
				type: "POST",
				url: "//" + USER_DOMAIN + '/api/ask/addAsk',
				data: {
					question_desc: content,
					email: submitType === 'email' ? ct_email : undefined,
					screenshot: sendimagefile,
					question_type: currentType,
					referer: window.location.origin + window.location.pathname,
					contact_id: contactId
				},
				dataType: "json",
				xhrFields: {
					withCredentials: true
				},
				success: function (res) {
					if (res.status == 1) {
						StatisticsDataWithComment(147, 4, '提交工单');
						imagefile = [];
						questionType = '';
						$('.image-content').remove();
						$('#iptArea').val('');
						$('#wechat-area').addClass('none');
						$('#email-area').addClass('none');
						$('[name="receiveMethod"]').prop('checked', false);
						$('#blog-feedback-modal').modal('hide');
						$('#feedback-modal-tips').modal('show');
					} else {
						showError(res.msg);
					}
				}
			});
		} else {
			showError('问题类型不能为空');
		}
	}

	function openQQHelp(url) {
		window.open(url);
		StatisticsDataWithComment(147, 4, 'qq咨询');
		$.ajax({
			type: "POST",
			url: "//" + USER_DOMAIN + '/api/ask/addAsk',
			data: {
				referer: window.location.origin + window.location.pathname,
				contact_id: isWorkTime ? undefined : (todayIsDayOff ? 10 : 9),
			},
			dataType: "json",
			xhrFields: {
				withCredentials: true
			},
			success: function (res) {
			}
		});
	}

	function getCookie(name) {
		var arr, reg = new RegExp("(^| )" + name + "=([^;]*)(;|$)");
		if (arr = document.cookie.match(reg))
			return unescape(arr[2]);
		else
			return null;
	}

	function resetShadowBox() {
		$('p img').each(function(i,item) {	
			var url;
			url = this.src; 

			$(item).mouseover(function(){
				$(this).css('cursor','pointer');
			}).mouseleave(function(){
				$(this).css('cursor','default');
			})
			
			if (! this.parentNode.href) {
				$(this).wrap("<a href='"+url+"' rel='shadowbox[sbalbum-81];player=img;'></a>");
			}
		});

		$('.gallery-item img').each(function(i,item){
			
			$(item).mouseover(function(){
				$(this).css('cursor','pointer');
			}).mouseleave(function(){
				$(this).css('cursor','default');
			})
		});
		if (typeof Shadowbox !== 'undefined') {
			Shadowbox.init({ skipSetup: true });
			Shadowbox.setup();
		}
	}

	function locationActiveNavigation(barHeight) {
		setTimeout(function() {
			var docSide = $('.doc-side')[0];
			var activeNav = $('.nav-link-post.active');
			if (activeNav.length > 0 && docSide) {
				var activeParent = activeNav.parents('.sub-nav');
				var docSideScrollTop = docSide.scrollTop;
				var subHeight = barHeight + 64 + 38 + 70 + 80;
				var scrollDistance = activeParent[0].getBoundingClientRect().top + docSideScrollTop - subHeight;
				docSide.scrollTo(0, scrollDistance);
			}
		}, 100);
	}
	
