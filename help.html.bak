                $('#support-wrapper-v3').removeClass('none');
            }
		});

		$('.images-add').click(function (event) {
    		$('#uploadImg').click();
		});
		  
		seajs.use(['//static.clewm.net/public/upload.js?kid=cliim', 'm/upload'], function (upss, upsss) {
			newUplode($('#uploadImg'), {
				"exts": "jpg,jpeg,png",
