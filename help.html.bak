				break;
			case 'email':
				submitType = 'email';
				$('#wechat-area').addClass('none');
				$('#email-area').removeClass('none');
				StatisticsDataWithComment(147, 3, '工单回复方式选择邮件接收');
				if (isWorkTimeFull) {
					$('#feedback-modal-tips-text').text('我们会在30分钟内通过邮件回复');
				} else {
					$('#feedback-modal-tips-text').text('我们会在工作时间通过邮件回复');
				}
				break;
			default:
				break;
			}
		});

		$('.collapse-sider-btn').click(function(){ $('.wrapper-container').removeClass('expanded') });
		$('.expand-sider-btn').click(function(){ $('.wrapper-container').addClass('expanded') });

		// 帮助中心页面的咨询随滚动出现
