						$('#blog-feedback-modal').modal('show');
					}
				} else {
					var currentEditionId = getCookie('login_edition_id') || '1';
					if (currentEditionId > 1) {
						if (isWorkTimeFull) {
							if (isWorkTime) {
								openQQHelp(helpQqUrl);
							} else {
								openQQHelp($ServiceTextGetter().getQQLink());
							}
						} else {
							$('#blog-feedback-modal').modal('show');
						}
					} else {
						$('#blog-feedback-modal').modal('show');
					}
				}
			}
		}, 2000));

		$('input[type=radio][name="receiveMethod"]').change(function() {
			switch (this.value) {
			case 'wechat':
				submitType = 'wechat';
				if (!$('#wechat-qr').attr('src')) {
