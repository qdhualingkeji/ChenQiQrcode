				};
			});

		$innerEditor.focus(function(e) {
			e.stopPropagation();
			if ($(this).val() && $('.matching-link').length) {
				$('.matching-data').show();
			}
		});

		$innerEditor.keydown(debounce(function (event) {
			var searchText = $(this).val();
			if ($.trim($(this).val()).length) {
				if (isInputZh) return;
				var searchResults = allPosts.filter(function(post) {
					return post.title.indexOf(searchText) > -1;
				});
				if (searchResults.length) {
					$('.matching-data').empty();
					searchResults.forEach(function (value, index) {
						$('.matching-data').append('<a href=' + value.url + ' class="matching-link">' + value.title + '</a>')
					});
					$('.matching-data').show();
				} else {
					$('.matching-data').empty().hide();
				}
			} else {
				$('.matching-data').empty().hide();
			}
		}, 300));

		$(document).on('click', 'a.nav-child-title', function(e) {
			e.preventDefault();
			$(this).next('.sub-child-nav').find('li:first-child a.nav-link-post').click();
		});

		$(document).on('click', 'a.nav-link.nav-link-post,.matching-link', function(e) {
			$('a.nav-link').removeClass('active');
			$('a.nav-title').removeClass('active');
			if ($(this).hasClass('nav-link-post')) {
				$(this).addClass('active');
				$(this).parents('.sub-nav').prev('.nav-title').addClass('active');
			}
			var _this = $(this);
			if (window.history.pushState) {
				e.preventDefault();
				if (!bodyLock) {
					bodyLock = true;
					NProgress.start();
					$.ajax({
						type: 'GET',
						url: _this.attr('href') + '?json=1',
						dataType: 'json',
						success: function (res) {
							bodyLock = false;
							NProgress.done();
  							// NProgress.remove();
							if (res.status === 'ok') {
								$('.matching-data').hide();
								$('.entry-title').text(res.post.title);
								$('.entry-content').html(res.post.content);
								window.scroll(0, 0);
								document.title = res.post.title + ' - 草料二维码';
								if (CURRENT_CATEGORY_TYPE == 'HELP') {
									window.history.pushState({}, res.post.title, '/help/' + res.post.id);
									// 百度统计
									_hmt.push(['_trackPageview', '/help/' + res.post.id]);
									var postCategories = res.post.categories.filter(function(item) {
										return item.id == RECORD_BLOG_CAT_ID || item.id == BATCH_BLOG_CAT_ID || item.id == SCENE_BLOG_CAT_ID || item.id == BUY_BLOG_CAT_ID;
									});
									if (postCategories.length) {
										reRenderSupport(postCategories[0].id + '');
										currentCategoryId = postCategories[0].id + '';
									} else {
										reRenderSupport(null);
										currentCategoryId = null;
									}
									if (_this.hasClass('matching-link')) {
										var navLinkPost = $('.nav-link-post[href="' + _this.attr('href') + '"]');
										navLinkPost.addClass('active');
										navLinkPost.parents('.sub-nav').collapse('show');
										navLinkPost.parents('.sub-nav').prev('.nav-title').addClass('active');
										locationActiveNavigation(adminBarHeight);
										StatisticsDataWithComment(147, res.post.id, res.post.title);
									} else {
										StatisticsDataWithComment(146, res.post.id, res.post.title);
									}
								} else {
									window.history.pushState({}, res.post.title, '/news/notice/' + res.post.id);
									if (_this.hasClass('matching-link')) {
										var navLinkPost = $('.nav-link-post[href="' + _this.attr('href') + '"]');
										navLinkPost.addClass('active');
										navLinkPost.parents('.sub-nav').collapse('show');
										navLinkPost.parents('.sub-nav').prev('.nav-title').addClass('active');
										locationActiveNavigation(adminBarHeight);
									} else {
										StatisticsDataWithComment(146, res.post.id, res.post.title);
									}
								}


                if (_this.attr('data-postid') != '48446' && window.location.pathname.indexOf('/notice/') === -1) {
                    var divEle = document.createElement("div");
                    divEle.setAttribute('style', 'margin-top: 80px;');
                    divEle.innerHTML = '<a class="text-blue" href="//cli.im/help/48446" style="font-size: 14px;">返回帮助中心首页 ></a>';
                    $('.entry-content').append(divEle);
                }

								resetShadowBox();
							} else {
								alert('加载失败');
							}
						},
						error: function() {
							NProgress.done();
  							// NProgress.remove();
							bodyLock = false;
							alert('加载失败');
						}
					});
				}
			} else {
				return true;
			}
		});

		// 监听返回修复浏览器没有更改内容
		window.onpopstate = function() {
			if (!location.hash) {
				location.reload();
			}
		}
	});
</script>

